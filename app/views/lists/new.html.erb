<h1><%= t("lists.views.new") %></h1>
<%= simple_form_for @list, :url => user_lists_path(current_user), :html => {:class => 'form-horizontal' } do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.button :submit, :class => 'btn btn-primary' %> 
  <%= link_to t('cancel'), user_lists_path(current_user) , :class => "btn" %>
<% end %>